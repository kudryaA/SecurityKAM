<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="256.0" prefWidth="367.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kam.design.contoller.SetPassword">
   <children>
      <Label layoutX="13.0" layoutY="14.0" text="Enter password">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <PasswordField fx:id="textFieldPassword" layoutX="14.0" layoutY="56.0" prefHeight="33.0" prefWidth="338.0">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="textFieldConfirm" layoutX="15.0" layoutY="147.0" prefHeight="33.0" prefWidth="338.0">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label layoutX="14.0" layoutY="105.0" text="Confirm password">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <ImageView fitHeight="60.0" fitWidth="50.0" layoutX="159.0" layoutY="192.0" onMouseClicked="#clickOk" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/ok.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
